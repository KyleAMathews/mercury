<VirtualHost *:8083>
	ServerAdmin webmaster@localhost
	ServerName dev.pantheon.*
	ServerAlias *.dev.pantheon.*

	DocumentRoot /var/www/html/pantheon/dev
	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory /var/www/html/pantheon/dev>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride All
		Order allow,deny
		allow from all
	</Directory>
</VirtualHost>
