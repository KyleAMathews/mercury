<VirtualHost *:8082>
	ServerAdmin webmaster@localhost
	ServerName test.pantheon.*
	ServerAlias *.test.pantheon.*

	DocumentRoot /var/www/html/pantheon/test
	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory /var/www/html/pantheon/test>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride All
		Order allow,deny
		allow from all
	</Directory>
</VirtualHost>
